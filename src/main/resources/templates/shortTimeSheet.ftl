<div class="product-list short-time-sheet time-sheet" data-menu-id="${menuItem.id}">
    <div class="product-list-title font-h2">
    ${menuItem.name}
    </div>

    <div>
        <div class="data-selector">
            <select class="selector">
                <option>30-05 травня</option>
                <option selected="selected">7-13 травня</option>
                <option>14-20 травня</option>
            </select>
        </div>


        <table>
            <tr>
                <th>Проект</th>
                <th>Країна</th>
                <th>Місто</th>
                <th class="time-cells">Пн - 07.05</th>
                <th class="time-cells">Вт - 08.05</th>
                <th class="time-cells">Ср - 09.05</th>
                <th class="time-cells">Чт - 10.05</th>
                <th class="time-cells">Пт - 11.05</th>
                <th class="time-cells">Сб - 12.05</th>
                <th class="time-cells">Нд - 13.05</th>
            </tr>
            <tr>
                <td>
                    <select class="selector">
                        <option selected="selected">Проект 1</option>
                        <option>Проект 2</option>
                        <option>Проект 3</option>
                    </select>
                </td>
                <td>
                    <select class="selector">
                        <option selected="selected">Україна</option>
                        <option>Канада</option>
                        <option>Ірак</option>
                    </select>
                </td>
                <td>
                    <select class="selector">
                        <option selected="selected">Київ</option>
                        <option>Ванкувер</option>
                        <option>Ірак</option>
                    </select>
                </td>
                <td class=" time-cells"> <input class="time-input" type="text"/> </td>
                <td class=" time-cells"> <input class="time-input" type="text"/> </td>
                <td class=" time-cells"> <input class="time-input" type="text"/> </td>
                <td class=" time-cells"> <input class="time-input" type="text"/> </td>
                <td class=" time-cells"> <input class="time-input" type="text"/> </td>
                <td class=" time-cells"> <input class="time-input" type="text"/> </td>
                <td class=" time-cells"> <input class="time-input" type="text"/> </td>
            </tr>

        </table>

        <div class="data-selector">
            <button type="button" class="btn btn-info open-cart-btn">
                Підтвердити
            </button>
        </div>


    </div>


</div>